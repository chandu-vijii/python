def length_of_last_word(s):
    words = s.split()
       if not words:
        return 0
    return len(words[-1])
s = "Hello World"
print(length_of_last_word(s))  # Output: 5
s = "Hello World   "
print(length_of_last_word(s))  # Output: 5 (ignoring trailing spaces)
s = "   "
print(length_of_last_word(s))  # Output: 0 (no words)
